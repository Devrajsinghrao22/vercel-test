{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        { "handle": "filesystem" },
    {
      "src": "api/routes/(.*)",
      "dest": "api/routes/$1"
    },
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ]
  }
  